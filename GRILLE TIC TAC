grille_tic_tac=[['_','_','_'],
                ['_','_','_'],
                ['_','_','_']]
print ('a toi joueur 1:')
colonne=int(input('choisis ta colonne?'))
ligne=int(input ('choisis ta ligne?'))
grille_tic_tac[ligne][colonne]='x'
print(grille_tic_tac)
print ('a toi joueur 2:')
colonne=int(input('choisis ta colonne?'))
ligne=int(input ('choisis ta ligne?'))
grille_tic_tac[ligne][colonne]='o'
print(grille_tic_tac)
